  echo "fa13e2e48e0b4c25c3ae38f1418d90b967001beedbc8891b6ccfcb9d54118630  OpenShot-v3.2.1-x86_64.exe" | shasum -a 256 --check

You should get the following output:
  OpenShot-v3.2.1-x86_64.exe: OK