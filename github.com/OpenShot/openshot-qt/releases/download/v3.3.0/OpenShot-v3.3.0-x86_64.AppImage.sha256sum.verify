  echo "3243347d4d3a8996b1a43fc2316830cc00ec1c9a9e2ec1d462c657e059cc8ec1  OpenShot-v3.3.0-x86_64.AppImage" | shasum -a 256 --check

You should get the following output:
  OpenShot-v3.3.0-x86_64.AppImage: OK